{
  "manifest_version": 3,
  "version": "1.0",
  "short_name": "Ghost Clean",
  "name": "Ghost Clean",
  "icons": {
    "512": "img/Logo.png"
  },
  "permissions": [
    "history",
    "cookies",
    "browsingData",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "512": "img/Logo.png"
    }
  }
}
